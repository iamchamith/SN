
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_View.cshtml";
}
<ul id="leftmenu" class="list-group" style="margin:0px;">
    <li class="list-group-item active list-group-edit menu" data-menu="preview" style="padding:10px; padding-left:20px; border-radius:0px;">
        <a href="#" data-toggle="tab" class="menu" data-menu="preview">Profile</a>
    </li>
    <li class="list-group-item list-group-edit menu" data-menu="ask" style="padding:10px; padding-left:20px; border-radius:0px;">
